CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
FRAMEWORK_SEARCH_PATHS = $(PLATFORM_DIR)/Developer/Library/Frameworks $(inherited) "$(SDKROOT)/Developer/Library/Frameworks" "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = -DKIF_XCTEST $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Alamofire.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/CYRTextView.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/JTSImageViewController.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/KIF.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Nimble.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Quick.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SSDataSources.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SnapKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/TTTAttributedLabel.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/pop.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "Alamofire" -framework "CYRTextView" -framework "JTSImageViewController" -framework "KIF" -framework "Nimble" -framework "Quick" -framework "SSDataSources" -framework "SnapKit" -framework "TTTAttributedLabel" -framework "pop"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-BequestTests
PODS_ROOT = ${SRCROOT}/Pods